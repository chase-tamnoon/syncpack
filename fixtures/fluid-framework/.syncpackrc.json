{
  "indent": "\\t",
  "customTypes": {
    "engines": {
      "path": "engines",
      "strategy": "versionsByName"
    },
    "packageManager": {
      "path": "packageManager",
      "strategy": "name@version"
    }
  },
  "source": [
    "**/package.json"
  ],
  "semverGroups": [
    {
      "dependencyTypes": [
        "dev"
      ],
      "range": "^"
    },
    {
      "dependencyTypes": [
        "prod"
      ],
      "range": ""
    }
  ],
  "versionGroups": [
    {
      "label": "Ignore TypeScript",
      "dependencies": [
        "*typescript*"
      ],
      "isIgnored": true
    },
    {
      "label": "Snap Biome",
      "dependencies": [
        "@biomejs/biome"
      ],
      "snapTo": [
        "@fluidframework/azure-local-service"
      ]
    },
    {
      "label": "Pin arethetypeswrong",
      "dependencies": [
        "@arethetypeswrong/cli"
      ],
      "pinVersion": "~0.2.2"
    }
  ]
}
